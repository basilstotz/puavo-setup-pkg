#!/bin/sh

set -eu



command=$1
shift



case "${command}" in


    configure)
	upstream_dir=$1

	"$upstream_dir"/amxabasis/install.sh

	;;
    
    unconfigure)
       true	
	;;
    
    unpack)
	upstream_pack=$1
	upstream_dir=$2

	tar -x -z -f "${upstream_pack}" -C "${upstream_dir}" 
	;;
    
    *)
	;;
esac
